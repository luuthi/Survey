<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - My Survey Online</title>
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")
    <!-- NProgress -->
    
    <!-- Datatables -->
    <link href="@Url.Content("~/Content/dataTables.bootstrap.min.css")" rel="stylesheet">
    <link href="@Url.Content("~/Content/dataTables.responsive.css")" rel="stylesheet">
    <link href="@Url.Content("~/Content/buttons.bootstrap.min.css")" rel="stylesheet">
    <link href="@Url.Content("~/Content/css-frontend/fixedHeader.bootstrap.min.css")" rel="stylesheet">
    <link href="@Url.Content("~/Content/css-frontend/responsive.bootstrap.min.css")" rel="stylesheet">
    <link href="@Url.Content("~/Content/css-frontend/scroller.bootstrap.min.css")" rel="stylesheet">
    <script src="~/Scripts/js-frontend/jquery.validate.min.js"></script>

</head>
<body>
   
    @Html.Partial("Menu_full")
    @Html.Partial("Menu_Responsive")
   
    <div class="container body-content col-md-12">
        @RenderBody()
    </div>

    <footer class="footer col-md-12 col-xs-12">
        @Html.Partial("Footer")
    </footer>
    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    @Scripts.Render("~/bundles/jscustomize")
    @RenderSection("scripts", required: false)
   
    <!-- Datatables -->
    <script src="@Url.Content("~/Scripts/jquery.dataTables.min.js")"></script>
    <script src="@Url.Content("~/Scripts/dataTables.bootstrap.min.js")"></script>
    <script src="@Url.Content("~/Scripts/dataTables.buttons.min.js")"></script>
    <script src="@Url.Content("~/Scripts/buttons.bootstrap.min.js")"></script>
    <script src="@Url.Content("~/Scripts/dataTables.responsive.min.js")"></script>
    <script src="@Url.Content("~/Scripts/responsive.bootstrap.js")"></script>
    <script src="@Url.Content("~/Scripts/dataTables.scroller.min.js")"></script>

    <script>
      
        $('#dataTables-example')
            .DataTable({
                responsive: true,
                "order": [[0, "desc"]]
            });
    </script>
</body>
</html>
